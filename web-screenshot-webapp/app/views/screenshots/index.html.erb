<p id="notice"><%= notice %></p>

<h1>Screenshots</h1>

<%= link_to 'New Screenshot', new_screenshot_path %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <ul>
    <% @images.each do |image| %>
      <li><%= link_to image, screenshot_path(key: image) %></li>
    <% end %>
  </ul>
</table>
